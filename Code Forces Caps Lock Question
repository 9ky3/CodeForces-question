x = str(input())
c = 0
b = True
l = []
for letter in x:
   if x.islower():
       print(x)
       b = False
       break


if b == True:
   for letter in x:
       if c == 0:
           l.append(letter.upper())
       else:
           l.append(letter.lower())
       c+=1
   y = ''.join(l)
   print(y)
